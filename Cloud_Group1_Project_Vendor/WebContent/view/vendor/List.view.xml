								showSuggestion="true" showValueHelp="true"
								valueHelpRequest="handleValueHelp"
								suggestionItems="{MainData>/MainData}"
								suggestionItemSelected="suggestionItemSelected">
								<suggestionItems>
									<core:ListItem key="{MainData>BNumber}" text="{MainData>BName}"
										additionalText="{MainData>BName}" />
								</suggestionItems>
							</Input>
						</fb:control>
					</fb:FilterItem>
					<fb:FilterItem name="Vceo" label="대표자">
